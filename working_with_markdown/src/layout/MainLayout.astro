---
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";
interface props{
    name: string;

}

const {name} = Astro.props as props
---
<html>
    <head>
        <title>{name}</title>
    </head>
    <body>
        <Header name={name}/>
        <slot />
        <Footer />
    </body>
</html>
<style>
    *{
        margin: 0;
        padding:0;
        box-sizing: border-box;
    }
</style>